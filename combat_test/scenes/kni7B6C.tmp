script/source = "extends Area2D

@export var speed = 400 # How fast the player will move (pixels/sec).
var screen_size # Size of the game window.
var direction
var is_attacking = false
var is_blocking = false
# Called when the node enters the scene tree for the first time.
func _ready():
	screen_size = get_viewport_rect().size #Vector de resoluciÃ³n de pantalla
	direction = str(\"down\") #El personaje empieza mirando hacia abajo
	position = screen_size / 2 #situamos al personaje en el centro de la pantalla

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):	
	move(delta)
	attack()
	block()

func move(delta):
	var velocity = Vector2.ZERO # The player's movement vector.
	if is_attacking == false && is_blocking == false:
		if Input.is_action_pressed(\"ui_right\"):
			velocity.x += 1
			direction = \"right\"
		if Input.is_action_pressed(\"ui_left\"):
			velocity.x -= 1
			direction = \"left\"
		if Input.is_action_pressed(\"ui_down\"):
			velocity.y += 1
			direction = \"down\"
		if Input.is_action_pressed(\"ui_up\"):
			velocity.y -= 1
			direction = \"up\"

		if velocity.length() > 0:
			velocity = velocity.normalized() * speed
			$KnightSprite.play(str(\"run_\" + direction))
		else:
			$KnightSprite.play(str(\"iddle_\" + direction))
		position += velocity * delta
		position = position.clamp(Vector2.ZERO, screen_size)
	
func attack():
	if Input.is_action_just_pressed(\"left\") && is_attacking == false && is_blocking == false:
		is_attacking = true
		$KnightSprite.play(str(\"attack_\" + direction))
		await get_tree().create_timer(0.5).timeout
		is_attacking = false

func block():
	if Input.is_action_just_pressed(\"right\") && is_attacking == false && is_blocking == false:
		is_blocking = true
		$KnightSprite.play(str(\"block_\" + direction))
		await get_tree().create_timer(0.8).timeout
		is_blocking = false
"

